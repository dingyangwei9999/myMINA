page{
  /*background-color: #191B1D;*/
}
.titlepic{
  padding: 50px 0;
}
image{
  display: block;
  margin: 0 auto;
}

audio{
  display: block;
  margin: 0 auto;
  margin-bottom: 30px;
  text-align: center;
  border: 36px solid #333232;
}
.control{
  padding: 20px 40px;
  /*border: 4px solid #333232;*/
  border-radius: 30px;
}
.start{
  color: #fff;
  background-color:  #5cb85c;
  display: inline-block;
  width: 80px;
  height: 80px;
  border-radius:50%;
  line-height: 80px;
  margin-right: 20px;
  font-weight: bold;
}
.pause{
  background-color: #777;
  display: inline-block;
  color: #fff;
  width: 80px;
  height: 80px;
  line-height: 80px;
  border-radius:50%;
  margin-right: 20px;
  font-weight: bold;
  
}
.replay{
  display: inline-block;
  background-color: #d9534f;
  color: #fff;
  width: 80px;
  height: 80px;
  line-height: 80px;
  border-radius:50%;
  font-weight: bold;
  
}